{
	"name": "@samplekit/preprocess-katex",
	"version": "2.0.0",
	"description": "Svelte preprocessor for TeX using KaTeX.",
	"author": "Tim Cohen <contact@timcohen.dev>",
	"license": "MIT",
	"type": "module",
	"scripts": {
		"format": "prettier --ignore-path ../../.prettierignore --check .",
		"format:fix": "prettier --ignore-path ../../.prettierignore --list-different --write .",
		"lint": "pnpm -w run lint:preprocess-katex",
		"lint:fix": "pnpm -w run lint:preprocess-katex:fix",
		"check": "tsc --noEmit",
		"validate": "pnpm install --frozen-lockfile && pnpm format && pnpm lint && pnpm check",
		"clean": "rm -rf dist",
		"build": "pnpm clean && tsc && cp src/katex.css dist && cp -r node_modules/katex/dist/fonts dist",
		"prepublishOnly": "pnpm build"
	},
	"exports": {
		".": "./dist/index.js",
		"./client": "./dist/client.js",
		"./katex.css": "./dist/katex.css",
		"./katex.js": "./dist/katex.js",
		"./package.json": "./package.json"
	},
	"peerDependencies": {
		"svelte": "^5.0.0"
	},
	"dependencies": {
		"estree-walker": "^3.0.3",
		"katex": "^0.16.11",
		"magic-string": "^0.30.12"
	},
	"devDependencies": {
		"@types/katex": "^0.16.7"
	},
	"homepage": "https://preprocessors.samplekit.dev",
	"repository": {
		"type": "git",
		"url": "https://github.com/timothycohen/samplekit/tree/main/packages/preprocess-katex"
	},
	"bugs": {
		"url": "https://github.com/timothycohen/samplekit/issues"
	},
	"keywords": [
		"latex",
		"katex",
		"tex",
		"math",
		"javascript",
		"typescript",
		"svelte",
		"sveltekit",
		"preprocess",
		"samplekit",
		"preprocess-katex",
		"svelte-pp-katex"
	]
}
